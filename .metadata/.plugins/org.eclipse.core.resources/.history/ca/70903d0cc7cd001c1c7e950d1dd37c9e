package dataBaseConnectivity;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.PreparedStatement;
import java.sql.SQLException;
import java.sql.Statement;

class FileStoreInADatabase {

	public static void main(String[] args) {
	       
		try {
			Connection mycon=DriverManager.getConnection("jdbc:oracle:thin:@locslhost:1521:xe","system","oracle");
			Statement state=mycon.createStatement();
			state.execute("create table file(id num(45),file clob)");
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}

	}

}
