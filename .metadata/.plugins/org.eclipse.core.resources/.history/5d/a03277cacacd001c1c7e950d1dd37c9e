package dataBaseConnectivity;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.PreparedStatement;
import java.sql.SQLException;
import java.sql.Statement;

class FileStoreInADatabase {

	public static void main(String[] args) {
		String sql="create table filetable(id number(38), file clob)";
	       
		try {
		
			Connection mycon=DriverManager.getConnection("jdbc:oracle:thin:@localhost:1521:xe","system","oracle");
			Statement state=mycon.createStatement();
			state.execute(sql);
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}

	}

}
